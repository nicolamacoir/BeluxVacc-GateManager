{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "camelcase": "off",
        "indent": [2, "tab"],
        "no-tabs":0,
        "no-plusplus":0,
        "func-names": 0,
        "no-console":0,
        "max-len":0,
        "no-await-in-loop":0,
        "no-unused-vars":0
    }
}
